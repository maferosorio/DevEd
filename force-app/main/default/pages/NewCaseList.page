<apex:page controller="NewCaseListController" >
    <apex:pageBlock title="Cases List" id="cases_list">
        <apex:repeat value="{! newCases }" var="case">
            <li>
                <apex:outputLink value="/{! case.Id }">
                <apex:outputText value="{! case.CaseNumber }"/>
                </apex:outputLink>
            </li>
        </apex:repeat>
    </apex:pageBlock>
</apex:page>